InstantKill.Name = "Instant Kill"
InstantKill.Parent = Combat
InstantKill.BackgroundColor3 = Color3.fromRGB(19, 19, 19)
InstantKill.BorderColor3 = Color3.fromRGB(19, 19, 19)
InstantKill.Position = UDim2.new(0.0319148935, 0, 0.561538458, 0)
InstantKill.Size = UDim2.new(0, 175, 0, 27)
InstantKill.Text = "Instant Kill"
InstantKill.TextColor3 = Color3.fromRGB(255, 255, 255)
InstantKill.TextSize = 14.000
InstantKill.MouseButton1Down:connect(function()
	local met = getrawmetatable(game)
	setreadonly(met,false)
	local old = met.__namecall
	met.__namecall = function(t,...)
		local args = {...}
		if(getnamecallmethod()=="FireServer") then
			if(tostring(t) == "HitPart") then
				for i = 0,25 do old(t,...)end
			end

		end
		return old(t,...)
	end

end)

